<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="kr.hs.mirim.young.applicationuitest.ui.home.HomeFragment"
        android:label="@string/title_Todo"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="kr.hs.mirim.young.applicationuitest.ui.dashboard.DashboardFragment"
        android:label="@string/title_schedule"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="kr.hs.mirim.young.applicationuitest.ui.notifications.NotificationsFragment"
        android:label="@string/title_group"
        tools:layout="@layout/fragment_notifications" />
</navigation>